    style(type='text/css').
        table.type11 {
            border-collapse: separate;
            border-spacing: 1px;
            text-align: center;
            line-height: 1.5;
            margin: 20px 10px;
        }
        table.type11 th {
            /*width: 155px;*/
            padding: 10px;
            font-weight: bold;
            vertical-align: middle;
            color: #fff;
            background: #ce4869 ;
            text-align: center;
        }
        table.type11 td {
            /*width: 155px;*/
            padding: 10px;
            vertical-align: middle;
            border-bottom: 1px solid #ccc;
            /*background: #eee;*/
        }

